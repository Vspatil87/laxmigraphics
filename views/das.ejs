                    <th>Invoice No</th>
                    <th>Due Date</th>
                  </thead>
                  <tbody>
                    <% for(var i=0 ; i < invoice.length ; i++){%>
                      <tr>
                        <td>
                          <%= invoice[i].invoice_no %>
                        </td>
                        <td>
                          <%= invoice[i].due_date %>
                        </td>
                        <%}%>
                      </tr>
                  </tbody>
                </table>
              </div>
              <!-- </div> -->
            </div>
            <script>
              $(document).ready(function () {
                $('#table').dataTable({
                  "bPaginate": false,
                  "bLengthChange": false,
                  "bFilter": true,
                  "bInfo": false,
                  "bAutoWidth": false
                });
              });
            </script>
